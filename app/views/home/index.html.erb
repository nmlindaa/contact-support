<section class="section">
  <div class="section-header">
    <h1>Dashboard</h1>
  </div>
  <% if user_signed_in? %> 
    <div class="card-wrapper">
      <div class="card">
        <div class="card-body">
          <h4>Total Profile: <%= @profiles.count %></h4>
        </div>
      </div>

      <div class="card">
        <div class="card-body">
          <h4>Online: <%= @online_profiles.count %></h4>
        </div>
      </div>
    </div>
  <% end %>
</section>